<?xml version="1.0"?>
<extension key="de.systopia.ceemes" type="module">
  <file>ceemes</file>
  <name>Ceemes Newsletter API</name>
  <description>Integration for the Ceemes newsletter tool</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
