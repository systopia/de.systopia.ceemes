    <url desc="Documentation">https://github.com/systopia/de.systopia.ceemes</url>
    <url desc="Support">https://github.com/systopia/de.systopia.ceemes/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
